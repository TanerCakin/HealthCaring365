    "/api/labs/orders/{id}": {
      "get": {
        "summary": "Get lab order by id",
        "tags": ["Labs"],
        "security": [{ "ApiKeyAuth": [] }],
        "parameters": [
          { "name": "id", "in": "path", "required": true, "schema": { "type": "string" } }
        ],
        "responses": {
          "200": {
            "description": "Lab order",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/LabOrder" }
              }
            }
          },
          "404": { "description": "Lab order not found" }
        }
      },
      "patch": {
        "summary": "Update lab order (status/note)",
        "tags": ["Labs"],
        "security": [{ "ApiKeyAuth": [] }],
        "parameters": [
          { "name": "id", "in": "path", "required": true, "schema": { "type": "string" } }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": { "type": "object" }
            }
          }
        },
        "responses": {
          "200": { "description": "Updated lab order" },
          "404": { "description": "Lab order not found" }
        }
      }
    },

    "/api/labs/results": {
      "get": {
        "summary": "List lab results",
        "tags": ["Labs"],
        "security": [{ "ApiKeyAuth": [] }],
        "parameters": [
          { "name": "patientId", "in": "query", "schema": { "type": "string" } }
        ],
        "responses": {
          "200": {
            "description": "Array of lab results",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": { "$ref": "#/components/schemas/LabResult" }
                }
              }
            }
          }
        }
      }
    },

    "/api/labs/results/{id}": {
      "get": {
        "summary": "Get lab result by id",
        "tags": ["Labs"],
        "security": [{ "ApiKeyAuth": [] }],
        "parameters": [
          { "name": "id", "in": "path", "required": true, "schema": { "type": "string" } }
        ],
        "responses": {
          "200": {
            "description": "Lab result",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/LabResult" }
              }
            }
          },
          "404": { "description": "Lab result not found" }
        }
      }
    },

    "/api/medications": {
      "get": {
        "summary": "List medications",
        "tags": ["Medications"],
        "security": [{ "ApiKeyAuth": [] }],
        "parameters": [
          { "name": "patientId", "in": "query", "schema": { "type": "string" } }
        ],
        "responses": {
